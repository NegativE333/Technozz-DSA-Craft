<%- include("header") %>

<div class="container mt-4">
    <h3>Booked Rooms</h3>
    <div class="container mt-4">
        <table class="table">
            <thead>
              <tr>
                <th scope="col">#</th>
                <th scope="col">Username</th>
                <th scope="col">Meeting Title</th>
                <th scope="col">Capacity</th>
                <th scope="col">Location</th>
                <th scope="col">Status</th>
                <th scope="col">Duration (hrs)</th>
              </tr>
            </thead>
            <tbody>
            <% for(let i = 0; i<bookedRooms.length; i++){ %>
                <tr>
                  <th scope="row"><%= bookedRooms[i].name %></th>
                  <td><%= bookedRooms[i].userName %></td>
                  <td><%= bookedRooms[i].meetingTitle %></td>
                  <td><%= bookedRooms[i].capacity %></td>
                  <td><%= bookedRooms[i].location %></td>
                  <td><%= bookedRooms[i].status %></td>
                  <td><%= bookedRooms[i].duration %></td>
                </tr>
            <% } %>
            </tbody>
          </table>
    </div>
</div>